{
    "name": "Color picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "icons": {
        "16":"/images/16.png",
        "32":"/images/32.png",
        "48":"/images/48.png"
    }
}